{
  "name": "@passerelle/enclosure-vue",
  "version": "0.0.1",
  "description": "passerelle enclosure module for vue3",
  "author": "mew-ton <mxl_usmarc@slmail.mewton.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:hacomono-lib/passerelle.git",
    "directory": "packages/enclosure-vue"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepare": "tsup",
    "dev": "tsup --watch --clean",
    "build": "vite",
    "preview": "run-p preview:*",
    "preview:enclosure": "vite ./playground --open",
    "preview:insider": "cd ../../tests/playground-insider && yarn serve",
    "test": "run-p test:*",
    "test:spec": "vitest",
    "test:type": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "devDependencies": {
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "typescript": "^5.1.6",
    "vitest": "^0.32.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5",
    "yarn-run-all": "latest"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  }
}
