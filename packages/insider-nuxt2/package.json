{
  "name": "@passerelle/insider-nuxt2",
  "version": "0.0.1",
  "description": "passerelle insider module for nuxt2",
  "repository": {
    "type": "git",
    "url": "git@github.com:hacomono-lib/passerelle.git",
    "directory": "packages/insider-nuxt2"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "test": "run-p test:*",
    "test:spec": "vitest run",
    "test:type": "tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^18",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "nuxt": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  }
}
